        <Grid Height="524">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="453*"/>
                <ColumnDefinition Width="451*"/>
            </Grid.ColumnDefinitions>
            <Grid.Style>
                <Style/>
