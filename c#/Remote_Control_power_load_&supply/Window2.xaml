            <Button x:Name="HMC" Content="" HorizontalAlignment="Center" Height="222" Margin="0,160,0,0" VerticalAlignment="Top" Width="398" BorderBrush="{x:Null}" Click="HMC_Click" >
                <Button.Background>
                    <ImageBrush ImageSource="/HMC803.jpg"/>
                </Button.Background>
            </Button>
            <Button x:Name="LD400" Content="" HorizontalAlignment="Left" Height="222" Margin="78,160,0,0" VerticalAlignment="Top" Width="373" BorderBrush="{x:Null}" Click="LD400_Click" Grid.Column="1">
                <Button.Background>
                    <ImageBrush ImageSource="/2444043-40.jpg"/>
                </Button.Background>
            </Button>
            <Button Content="◀" HorizontalAlignment="Left" Height="68" Margin="-33,10,0,0" VerticalAlignment="Top" Width="98" Background="{x:Null}" BorderThickness="0,0,0,0" FontWeight="Bold" FontSize="48" Click="Button_Click" Foreground="#FF0024D3"/>
