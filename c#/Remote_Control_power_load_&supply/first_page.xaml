<Window x:Class="Control_Rohde_Schwarz.first_page"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Control_Rohde_Schwarz"
        mc:Ignorable="d"
        Title="first_page" Height="600" Width="1000" WindowStartupLocation="CenterScreen" Icon="/icons8-student-50.png">
    <Viewbox>
        <Grid x:Name="get_in_grid">
            <Image HorizontalAlignment="Center" Height="584" VerticalAlignment="Center" Width="1110" Source="/Banner_VSL_campaign_2022_text_layer.jpg" Stretch="UniformToFill"/>
            <Button x:Name="get_in" Content="START" HorizontalAlignment="Left" Margin="52,489,0,0" VerticalAlignment="Top" Height="70" Width="187" Foreground="White" FontSize="48" Background="{x:Null}" FontWeight="Bold" BorderBrush="#FF151515" Click="get_in_Click" BorderThickness="0,0,0,0">
                <Button.Effect>
                    <DropShadowEffect/>
                </Button.Effect>
            </Button>
        </Grid>
    </Viewbox>
</Window>
